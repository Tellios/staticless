{
  "name": "staticless",
  "version": "0.0.0",
  "description": "",
  "main": "dist/run.js",
  "scripts": {
    "lint": "tslint --fix src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "cleanDist": "rimraf dist",
    "prebuild:prod": "npm run cleanDist",
    "build:prod": "tsc && webpack --env.prod",
    "prebuild:watch": "npm run cleanDist",
    "build:watch": "concurrently \"tsc --watch\" \"webpack --watch --env.prod=false\" ",
    "start": "node --inspect-brk=5858 --no-lazy dist/run",
    "start:debug": "npm run build && npm run start"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/server/__tests__/setupTestFramework.ts",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*\\.test)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tellios/staticless.git"
  },
  "author": "Sonny Johannesson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tellios/staticless/issues"
  },
  "homepage": "https://github.com/Tellios/staticless#readme",
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/fs-extra": "^4.0.7",
    "@types/hapi": "^16.1.12",
    "@types/highlight.js": "^9.12.2",
    "@types/inert": "^4.2.5",
    "@types/jest": "^22.0.1",
    "@types/klaw-sync": "^1.1.3",
    "@types/lodash": "^4.14.92",
    "@types/marked": "^0.3.0",
    "@types/nconf": "0.0.35",
    "@types/node": "^8.5.8",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/superagent": "^3.5.6",
    "@types/winston": "^2.3.7",
    "awesome-typescript-loader": "^3.4.1",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.8",
    "jest": "^22.0.6",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.10",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.1",
    "ts-jest": "22.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
