{
  "name": "staticless",
  "version": "0.0.0",
  "description": "",
  "main": "dist/run.js",
  "scripts": {
    "pretty": "prettier --config .prettierrc.json --write \"src/**/*.{ts,tsx}\"",
    "pretty-ci": "prettier --config .prettierrc.json --list-different \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "precommit": "npm run pretty && npm run test && npm run build:prod",
    "cleanDist": "rimraf dist",
    "prebuild:prod": "npm run cleanDist",
    "build:prod": "tsc -p tsconfig.build.json && webpack --env.prod",
    "prebuild": "npm run cleanDist",
    "build": "tsc -p tsconfig.build.json && webpack --env.prod=false",
    "prebuild:watch": "npm run cleanDist",
    "build:watch": "concurrently \"tsc -p tsconfig.build.json --watch\" \"webpack --watch --env.prod=false\" ",
    "start": "node --inspect-brk=5858 --no-lazy dist/run",
    "start:debug": "npm run build && npm run start"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/server/__tests__/setupTestFramework.ts",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*\\.test)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tellios/staticless.git"
  },
  "author": "Sonny Johannesson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tellios/staticless/issues"
  },
  "homepage": "https://github.com/Tellios/staticless#readme",
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/fs-extra": "^5.0.4",
    "@types/hapi": "^17.6.2",
    "@types/highlight.js": "^9.12.2",
    "@types/inert": "^5.1.2",
    "@types/jest": "^23.3.7",
    "@types/jss": "^9.5.2",
    "@types/klaw-sync": "^5.0.0",
    "@types/lodash": "^4.14.107",
    "@types/marked": "^0.4.2",
    "@types/nconf": "0.0.37",
    "@types/node": "^10.12.0",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.19",
    "@types/redux-logger": "^3.0.5",
    "@types/superagent": "^3.5.8",
    "@types/winston": "^2.3.9",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^4.0.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^1.0.0",
    "jest": "^23.6.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.12.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.3",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@material-ui/core": "^3.3.1",
    "classnames": "^2.2.5",
    "fs-extra": "^7.0.0",
    "hapi": "^17.6.0",
    "hapi-async-handler": "^1.0.3",
    "highlight.js": "^9.12.0",
    "inert": "^5.1.0",
    "inversify": "^5.0.1",
    "klaw-sync": "^6.0.0",
    "lodash": "^4.17.5",
    "marked": "^0.5.1",
    "mermaid": "^8.0.0-rc.8",
    "nconf": "^0.10.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.12",
    "superagent": "^4.0.0-beta.5",
    "winston": "^3.1.0"
  }
}
