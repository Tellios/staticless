    "highlight.js": "^9.12.0",
    "inert": "^4.2.1",
    "inversify": "^4.9.0",
    "klaw-sync": "^3.0.2",
    "lodash": "^4.17.4",
    "marked": "^0.3.12",
    "material-ui": "^1.0.0-beta.27",
    "nconf": "^0.9.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "reflect-metadata": "^0.1.10",
    "superagent": "^3.8.2",
    "winston": "^2.4.0"
  }
