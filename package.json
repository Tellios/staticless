{
  "name": "staticless",
  "version": "0.0.0",
  "description": "",
  "main": "dist/run.js",
  "scripts": {
    "pretty": "prettier --config .prettierrc.json --write \"src/**/*.{ts,tsx}\"",
    "pretty-ci": "prettier --config .prettierrc.json --list-different \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "precommit": "npm run pretty && npm run test && npm run build:prod",
    "cleanDist": "rimraf dist",
    "prebuild:prod": "npm run cleanDist",
    "build:prod": "tsc -p tsconfig.build.json && webpack --env.prod",
    "prebuild:watch": "npm run cleanDist",
    "build:watch": "concurrently \"tsc -p tsconfig.build.json --watch\" \"webpack --watch --env.prod=false\" ",
    "start": "node --inspect-brk=5858 --no-lazy dist/run",
    "start:debug": "npm run build && npm run start"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/server/__tests__/setupTestFramework.ts",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*\\.test)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tellios/staticless.git"
  },
  "author": "Sonny Johannesson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tellios/staticless/issues"
  },
  "homepage": "https://github.com/Tellios/staticless#readme",
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/fs-extra": "^4.0.8",
    "@types/hapi": "^16.1.14",
    "@types/highlight.js": "^9.12.2",
    "@types/inert": "^4.2.7",
    "@types/jest": "^22.2.3",
    "@types/jss": "^9.5.2",
    "@types/klaw-sync": "^1.1.3",
    "@types/lodash": "^4.14.107",
    "@types/marked": "^0.3.0",
    "@types/nconf": "0.0.35",
    "@types/node": "^8.10.8",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.16",
    "@types/redux-logger": "^3.0.5",
    "@types/superagent": "^3.5.8",
    "@types/winston": "^2.3.9",
    "awesome-typescript-loader": "^3.5.0",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "jest": "^22.4.3",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.3",
    "prettier": "^1.12.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.1",
    "ts-jest": "22.0.1",
    "typescript": "^2.8.1",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "fs-extra": "^4.0.3",
    "hapi": "^16.6.3",
    "hapi-async-handler": "^1.0.3",
    "highlight.js": "^9.12.0",
    "inert": "^4.2.1",
    "inversify": "^4.11.1",
    "klaw-sync": "^3.0.2",
    "lodash": "^4.17.5",
    "marked": "^0.3.19",
    "material-ui": "^1.0.0-beta.42",
    "mermaid": "^7.1.2",
    "nconf": "^0.9.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reflect-metadata": "^0.1.12",
    "superagent": "^3.8.2",
    "winston": "^2.4.1"
  }
}
